bill = float(input("Input cost here"))
tip = (input("Input if service was bad, okay, good or great here"))
z = 1
a = 2
b = 3
def totalcost():
    if tip == ("bad"):tip == 0
    if tip == ("okay"):tip == 15
    if tip == ("good"):tip == 20
    if tip == ("great"):tip == 25
    else:print("invalid input")
    z = float(bill * 0.01)
    a = float(z * tip)
    b = int(bill + a)
    print(b)
totalcost()

